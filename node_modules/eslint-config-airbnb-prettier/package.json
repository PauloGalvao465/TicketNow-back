{
  "name": "eslint-config-airbnb-prettier",
  "version": "1.1.0",
  "description": "The Airbnb styleguide, but with Prettier.",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "author": "Ian Christian Myers <ian@iancmyers.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0"
  },
  "peerDependencies": {
    "eslint": "^4.18.1"
  }
}
